{
    "name": "chojnicki/video-platforms-parser",
    "description": "Easy to use SDK with grabber for multiple platforms at once like YouTube, Dailymotion, Facebook and more.",
    "type": "library",
    "license": "MIT",
    "keywords": ["parser", "grabber", "youtube", "dailymotion", "facebook", "video", "liveleak", "cda", "laravel"],
    "authors": [
        {
            "name": "chojnicki",
            "email": "lukchojnicki@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-json": "*",
        "ext-iconv": "*"
    },
    "autoload": {
        "psr-4": {
            "Chojnicki\\VideoPlatformsParser\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chojnicki\\VideoPlatformsParser\\ServiceProvider"
            ],
            "aliases": {
                "VideoPlatformsParser": "Chojnicki\\VideoPlatformsParser\\Facade"
            }
        }
    }
}
